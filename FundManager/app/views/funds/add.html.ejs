<div class="hero-unit">
  <form id="fund-form" class="form-horizontal" action="/funds" method="POST">
    <fieldset>
      <legend>总资产录入</legend>
      <% if(params.errors) { %>
      <div class="control-group">
        <ul>
        <% for(var err in params.errors) { %>
          <li><%= params.errors[err]; %></li>
        <% } %>
        </ul>
      </div>
      <% } %>

		 <%- partial('form', {fund: fund}) %> 
      
      <div class="form-actions">
        <%- contentTag('input', 'Add', {type: 'submit', class: 'btn btn-primary'}) %>
      </div>
    </fieldset>
  </form>
</div>
